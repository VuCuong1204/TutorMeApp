        <attr name="text_hint" format="string" />
        <attr name="padding_right_icon" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleInSideView">
        <attr name="center_icon" format="reference" />
        <attr name="center_icon_padding" format="dimension" />
